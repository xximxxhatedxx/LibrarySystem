      </Label>
      <PasswordField fx:id="passwordLogInField" layoutX="231.0" layoutY="170.0" styleClass="input" stylesheets="@style.css">
      <cursor>
         <Cursor fx:constant="TEXT" />
      </cursor></PasswordField>
      <Button fx:id="goToRegistrationButton" layoutX="199.0" layoutY="327.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="202.0" styleClass="inactivenButton" stylesheets="@style.css" text="Registration">
